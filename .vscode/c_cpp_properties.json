{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/libft.h",
        "/usr/include",
        "/usr/include/x86_64-linux-gnu"
      ],
      "defines": [
        "_POSIX_C_SOURCE=200809L"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c99",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-I.",
        "-I./libft"
      ]
    }
  ],
  "version": 4
}